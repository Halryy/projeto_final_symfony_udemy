{% extends 'base-public.html.twig' %}

{% block title %}Hello HomeController!{% endblock %}

{% block body %}
    {% for banner in banners %}
        <img src="{{ vich_uploader_asset(banner, 'imageFile') }}" width="75"><br>
    {% endfor %}

    {% for productCategory in productCategories %}
        {{ productCategory.title }}<br>
    {% endfor %}

    {% for news in allNews %}
        {{ news.title }}<br>
    {% endfor %}

    {% for testimony in testimonies %}
        {{ testimony.title }}<br>
    {% endfor %}

    {% for financing in financings %}
        {{ financing.title }}<br>
    {% endfor %}

    {{ pageSeo.homePageTitle }}<br>
    {{ pageSeo.homePageDescription }}<br>
    {{ generalData.address }}<br>
    {{ generalData.email }}<br>
    {{ generalData.phone }}<br>
    {{ globalTags.pixelMetaAds }}<br>
    {{ globalTags.tagsGoogleAds }}<br>
    {{ globalTags.ga4 }}<br>
    {{ urlToPostForm.url }}<br>

{% endblock %}
