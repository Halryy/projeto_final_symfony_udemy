<form method="post" action="{{ path('app_admin_news_delete', {'id': news.id}) }}" onsubmit="return confirm('Tem certeza que quer deletar este item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ news.id) }}">
    <button class="btn btn-danger float-start ms-3 text-white">Deletar</button>
</form>
